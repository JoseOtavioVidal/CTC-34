digraph Automatum {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	"q0,q3,q4"
	"q0,q1,q2"
	node [shape=circle]
	q0 -> "q0,q3" [label=1]
	q0 -> "q0,q1" [label=0]
	"q0,q3" -> "q0,q3,q4" [label=1]
	"q0,q3" -> "q0,q1" [label=0]
	"q0,q1" -> "q0,q3" [label=1]
	"q0,q1" -> "q0,q1,q2" [label=0]
	"q0,q3,q4" -> "q0,q3,q4" [label=1]
	"q0,q3,q4" -> "q0,q1" [label=0]
	"q0,q1,q2" -> "q0,q3" [label=1]
	"q0,q1,q2" -> "q0,q1,q2" [label=0]
}
